/* ======================================= */
/* GNU plot launcher                       */

void GnuPlot() {

char cmd[LN];

   sprintf(cmd,"xterm 2>/dev/null -e /usr/local/bin/gnuplot");
   printf("Launching: %s\n",cmd);
   Launch(cmd);
}

